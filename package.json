{
  "name": "nuxt-version-shake",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.12.1",
  "description": "Easy nuxt version checks with tree-shaking",
  "author": "Matthew Messinger <mattmess1221@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattmess1221/nuxt-version-shake"
  },
  "bugs": "https://github.com/mattmess1221/nuxt-version-shake/issues",
  "keywords": [
    "nuxt",
    "semver",
    "unplugin",
    "utility"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "module": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": ">=3.17.5",
    "consola": "^3.4.2",
    "magic-string": "^0.30.17",
    "oxc-walker": "^0.3.0",
    "semver": "^7.7.2",
    "unplugin": "^2.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/test-utils": "^3.19.1",
    "@types/node": "^24.0.3",
    "@types/semver": "^7.7.0",
    "@vitest/coverage-v8": "3.2.3",
    "changelogen": "^0.6.1",
    "eslint": "^9.29.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^16.1.2",
    "nuxt": "^3.17.5",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.3",
    "vue-tsc": "^2.2.10"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
